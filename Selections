-- SELECT the country column FROM the eurovision table
SELECT
   country
FROM
   eurovision;
    
    
-- Select the points column
Select
  points
FROM
  eurovision;
  
  
-- Limit the number of rows returned to the first 50
SELECT 
  top(50) points 
FROM 
  eurovision;
   
   
-- Return unique countries and use an alias
SELECT 
  distinct country as unique_country 
FROM 
  eurovision;
